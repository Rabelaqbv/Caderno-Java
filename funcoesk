fun main(args: Array<String>) {
  //criar uma variav√©l do tipo booleanna(false true)
  var nomeVariavel: Boolean = false
   println(nomeVariavel)
  var x: Boolean = true
   println(x)
  //if faz testee e retorna boolean

  if (x){
    println("verdadeiro")
   }
else{
     println("falso")
  }
  
}
